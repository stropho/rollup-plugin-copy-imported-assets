{
  "name": "rollup-plugin-copy-imported-assets",
  "version": "1.0.0",
  "description": "Copy asssets imported as es-modules",
  "main": "dist/copy-imported-assets.cjs.js",
  "module": "dist/copy-imported-assets.esm.js",
  "scripts": {
    "build": "npx rollup -c=rollup.config.js",
