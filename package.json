    "@babel/preset-env": "^7.5.5",
    "@babel/runtime": "^7.5.5",
    "babel-jest": "^24.8.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "jest": "^24.8.0",
    "rollup": "^1.19.4",
    "rollup-plugin-babel": "^4.3.3"
  },
  "dependencies": {
    "rollup-pluginutils": "^2.8.1"
  }
